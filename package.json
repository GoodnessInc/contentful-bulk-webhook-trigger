{
  "name": "@goodness.inc/contentful-bulk-webhook-trigger",
  "version": "0.0.2",
  "description": "Given a Contentful contentType, iterate through each entry and invoke provided webhook with a payload that is similar to Contentful webhooks",
  "repository": "github:GoodnessInc/contentful-bulk-webhook-trigger",
  "homepage": "https://github.com/GoodnessInc/contentful-bulk-webhook-trigger",
  "author": "Goodness Inc.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "contentful-bulk-webhook-trigger": "./dist/index.js"
  },
  "scripts": {
    "dev": "yarn build && tsc --watch",
    "format": "prettier --write .",
    "build": "tsc && chmod +x ./dist/index.js",
    "preversion": "npm run build"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.0.1",
    "contentful": "^11.9.0"
  },
  "devDependencies": {
    "@shopify/prettier-config": "^1.1.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prettier": "@shopify/prettier-config"
}
